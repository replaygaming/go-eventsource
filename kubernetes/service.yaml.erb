apiVersion: v1
kind: Service
metadata:
  name: eventsource
  labels:
    environment: <%= environment %>
  annotations:
    beta.cloud.google.com/backend-config: '{"ports": {"80":"eventsource"}}'
spec:
  type: NodePort
  selector:
    component: eventsource
    environment: <%= environment %>
  ports:
    - name: sse
      port: 80
      targetPort: 80
      protocol: TCP
  sessionAffinity: None
